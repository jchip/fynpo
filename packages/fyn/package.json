{
  "name": "fyn",
  "version": "2.0.0",
  "description": "fyn is the NPM for fynpo -- a zero setup monorepo tool",
  "main": "./bin/fyn.js",
  "homepage": "https://jchip.github.io/fynpo/",
  "license": "Apache-2.0",
  "scripts": {
    "test": "xrun xarc/test-only",
    "debug-test": "node --inspect-brk node_modules/.bin/mocha --extension ts,js,tsx,jsx,cjs,mjs -c test/spec",
    "lint": "xrun xarc/lint",
    "compile-yarn": "babel yarn/src --out-dir yarn/lib",
    "build": "xrun --serial fyn/create-tgz compile-yarn bundle",
    "analyze": "ANALYZE_BUNDLE=1 xrun compile-yarn bundle",
    "coverage": "xrun xarc/check",
    "ci:check": "xrun xarc/check",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublishOnly": "xrun build",
    "prepack": "publish-util-prepack",
    "postpack": "publish-util-postpack",
    "blah": "exit 5",
    "test-npm-a": "npm run test-npm-b",
    "test-npm-b": "env",
    "hello1": "npm run hello -- from npm",
    "hello": "echo hello"
  },
  "bin": {
    "0-npx-please-run-this-for-fyn": "./bin/fyn.js",
    "fyn": "./bin/fyn.js",
    "fun": "./bin/fun.js"
  },
  "author": "Walmart GTP.js Team and Contributors",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/",
    "tag": "v2"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "keywords": [
    "node",
    "node_modules",
    "module",
    "modules",
    "fast",
    "package",
    "manager",
    "enhanced",
    "local",
    "better",
    "improve",
    "npm",
    "link",
    "development",
    "workflow",
    "productivity",
    "efficient",
    "disk",
    "space",
    "usage",
    "monorepo",
    "mono-repo",
    "workspaces"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/fynpo.git",
    "directory": "packages/fyn"
  },
  "publishUtil": {
    "remove": [
      "dependencies"
    ],
    "keep": [
      "opfs"
    ]
  },
  "contributors": [
    "Joel Chen",
    "Dat Vong",
    "Divya Karippath"
  ],
  "dependencies": {
    "@fynpo/base": "^1.1.20",
    "@jchip/error": "^1.0.3",
    "@npmcli/arborist": "^9.1.6",
    "@npmcli/run-script": "^10.0.2",
    "aveazul": "^1.0.2",
    "bluebird": "^3.7.2",
    "cacache": "^20.0.1",
    "chalk": "^4.1.2",
    "check-pkg-new-version-engine": "^1.0.3",
    "ci-info": "^4.0.0",
    "filter-scan-dir": "^1.5.5",
    "ini": "^2.0.0",
    "init-package": "^1.0.0",
    "invariant": "^2.2.4",
    "item-queue": "^1.1.2",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.21",
    "make-fetch-happen": "^15.0.2",
    "minimatch": "^3.0.4",
    "mississippi": "^4.0.0",
    "nix-clap": "^2.4.4",
    "npm-packlist": "^10.0.3",
    "npm-registry-fetch": "^19.1.0",
    "opfs": "^1.1.1",
    "optional-require": "^1.1.10",
    "pacote": "^21.0.3",
    "pkg-preper": "^0.1.4",
    "publish-util": "^2.1.0",
    "semver": "^7.3.5",
    "source-map": "^0.7.3",
    "strip-bom": "^4.0.0",
    "tar": "^4.4.19",
    "visual-exec": "^0.1.14",
    "visual-logger": "^1.1.3",
    "xaa": "^1.8.0",
    "xenv-config": "^1.3.0",
    "xsh": "^0.4.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.16.0",
    "@xarc/module-dev": "^3.0.0",
    "agentkeepalive": "^3.5.3",
    "aveazul": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "electrode-server": "^3.0.3",
    "eslint": "^7.16.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^30.7.9",
    "mocha": "^8.2.1",
    "mock-require": "^2.0.2",
    "node-fetch-npm": "^2.0.4",
    "null-loader": "^4.0.1",
    "nyc": "^15.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^1.19.1",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.5.0",
    "tsx": "^4.20.3",
    "v8-compile-cache": "^2.3.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "which": "^2.0.2",
    "xstdout": "^0.1.1"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*/pkg-bin-linker-win32.js",
      "*clap.js",
      "*clap.ts",
      "bin",
      "cli/config",
      "cli/fun.js",
      "lib/util/file-ops-win32.js",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "stubs",
      "test",
      "tmp",
      "webpack.config.js",
      "xout",
      "xrun*.js",
      "xrun*.ts",
      "**/.eslint*.*",
      "yarn"
    ],
    "extends": [],
    "check-coverage": true,
    "statements": 76,
    "branches": 68,
    "functions": 79,
    "lines": 77,
    "cache": true
  },
  "engines": {
    "node": ">=14.14"
  },
  "opfs": {
    "bluebird": true
  },
  "prettier": {
    "printWidth": 100
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "mocha"
    ]
  },
  "mocha": {
    "require": [
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "fyn": {
    "dependencies": {
    }
  }
