{
  "name": "fynpo",
  "version": "2.0.1",
  "description": "fynpo is a zero setup monorepo manager for node.js",
  "main": "dist/index.js",
  "homepage": "https://jchip.github.io/fynpo/",
  "scripts": {
    "build": "rm -rf dist && webpack",
    "docs": "xrun xarc/docs",
    "test": "vitest run",
    "prepublishOnly": "xrun --serial [build, test]",
    "prepack": "publish-util-prepack",
    "postpack": "publish-util-postpack",
    "start": "tsx src/fynpo.ts"
  },
  "keywords": [
    "lerna",
    "mono",
    "repo",
    "mono-repo",
    "workspace",
    "monorepo",
    "javascript",
    "npm",
    "build"
  ],
  "files": [
    "dist",
    "lib",
    "templates"
  ],
  "author": "Walmart GTP.js Team and Contributors",
  "contributors": [
    "Joel Chen",
    "Divya Vannilaparambath"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/fynpo.git",
    "directory": "packages/fynpo"
  },
  "bin": {
    "fynpo": "bin/fynpo.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/",
    "tag": "v2"
  },
  "publishUtil": {
    "fynIgnore": true,
    "remove": [
      {
        "dependencies": [
          "/.*/"
        ]
      }
    ],
    "keep": [
      {
        "dependencies": [
          "fyn",
          "import-fresh",
          "resolve-global",
          "prettier"
        ]
      }
    ]
  },
  "dependencies": {
    "@commitlint/execute-rule": "^18.6.1",
    "@commitlint/format": "^18.6.1",
    "@commitlint/lint": "^18.6.1",
    "@commitlint/read": "^18.6.1",
    "@commitlint/resolve-extends": "^18.6.1",
    "@fynpo/base": "^1.1.21",
    "@jchip/error": "^1.0.3",
    "aveazul": "^1.0.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "chalker": "^1.2.0",
    "cosmiconfig": "^7.0.0",
    "deep-object-diff": "^1.1.7",
    "env-paths": "^2.2.1",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "fyn": "^2.0.1",
    "get-stdin": "^8.0.0",
    "import-fresh": "3.3.0",
    "item-queue": "^1.1.2",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "mkdirp": "^1.0.4",
    "nix-clap": "^2.4.5",
    "npm-packlist": "jchip/npm-packlist#v1",
    "optional-require": "^1.1.10",
    "prettier": "2.4.1",
    "regenerator-runtime": "^0.13.9",
    "resolve-from": "^5.0.0",
    "resolve-global": "1.0.0",
    "semver": "^7.3.5",
    "shcmd": "^0.8.4",
    "slash": "^3.0.0",
    "strong-log-transformer": "^2.1.0",
    "tslib": "^2.3.1",
    "undici": "^4.14.1",
    "visual-exec": "^0.1.14",
    "visual-logger": "^1.1.3",
    "xaa": "^1.7.2",
    "xsh": "^0.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/node": "^16.9.6",
    "babel-loader": "^8.0.4",
    "publish-util": "^2.0.0",
    "source-map-support": "^0.5.19",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "prettier": {
    "printWidth": 100
  }
}
